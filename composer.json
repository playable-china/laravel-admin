{
    "name": "playable-china/laravel-admin",
    "description": "laravel admin",
    "type": "library",
    "keywords": ["laravel", "admin", "grid", "chart", "form"],
    "homepage": "https://github.com/playable-china/laravel-admin",
    "license": "MIT",
    "authors": [
        {
            "name": "playable",
            "email": "info@playable.cn"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "~5.3",
        "intervention/image" : "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "laravel/laravel": "~5.3",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*",
        "fzaninotto/faker": "~1.4"
    },
    "autoload": {
        "psr-4": {
            "Encore\\Admin\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Models\\": "tests/models",
            "Tests\\Controllers\\": "tests/controllers"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    }
}
